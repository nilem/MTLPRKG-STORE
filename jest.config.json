{
  "testEnvironment": "node",
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  },
  "transform": {},
  "testMatch": [
    "**/__tests__/**/*.test.js",
    "**/?(*.)+(spec|test).js"
  ],
  "collectCoverageFrom": [
    "src/**/*.js",
    "!src/**/*.test.js",
    "!src/**/__tests__/**"
  ]
}
